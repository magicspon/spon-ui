{
	"name": "spon-ui",
	"description": "Gulp tasks and Fractal",
	"version": "3.0.0",
	"main": "gulpfile.js",
	"author": "Dave Stockley",
	"license": "MIT",
	"engines": {
		"node": ">=8.6.0"
	},
	"bugs": {
		"url": "https://github.com/magicspon/spon-ui/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/magicspon/spon-ui.git"
	},
	"browser": true,
	"jest": {
		"testURL": "http://localhost:3000/",
		"setupFiles": [
			"<rootDir>/setup-jest.js"
		],
		"mapCoverage": true,
		"collectCoverageFrom": [
			"src/js/core/**/*.{js,jsx,ts,tsx}"
		],
		"testMatch": [
			"**/?(*.)(spec|test).js?(x)"
		]
	},
	"quokka": {
		"babel": true,
		"installPackageCommand": "yarn add {packageName}",
		"plugins": [
			"jsdom-quokka-plugin"
		]
	},
	"homepage": "https://github.com/magicspon/spon-ui#readme",
	"scripts": {
		"start": "node_modules/.bin/gulp",
		"cms": "node_modules/.bin/gulp --config cms",
		"build:components": "node_modules/.bin/gulp build:partials",
		"build": "node_modules/.bin/gulp build --env production --config cms",
		"build:fractal": "node_modules/.bin/gulp build --env production --config fractal",
		"deploy": "netlify deploy",
		"test": "jest",
		"test:watch": "jest --watch",
		"coverage": "jest --coverage",
		"gen-cert": "mkdir private && node ./scripts/utils/cert/cli.js gen-certs",
		"lint:js": "eslint --ext src/js/** .",
		"lint:scss": "stylelint src/**/*.scss --syntax scss",
		"pretty": "prettier 'src/**/*.{js,jsx,scss}' --write",
		"generate-docs": "node_modules/.bin/jsdoc -c jsdoc.json && cd docs && http-server",
		"deploy-docs": "node_modules/.bin/jsdoc -c jsdoc.json && cd docs && netlify deploy"
	},
	"dependencies": {
		"critical": "^1.3.4",
		"debug": "^3.1.0",
		"dom-delegate": "^2.0.3",
		"domify": "^1.4.0",
		"fg-loadcss": "^2.0.1",
		"history": "^4.7.2",
		"intersection-observer": "^0.5.0",
		"lazysizes": "^4.0.2",
		"loadjs": "^3.5.4",
		"local-links": "^1.4.1",
		"mitt": "^1.1.3",
		"path-to-regexp": "^2.1.0",
		"picturefill": "^3.0.2",
		"plyr": "^3.3.21",
		"raf-throttle": "^2.0.3",
		"ramda": "^0.25.0",
		"spon-slide": "^1.0.8",
		"validate.js": "^0.12.0"
	},
	"devDependencies": {
		"@babel/core": "^7.0.1",
		"@babel/plugin-proposal-class-properties": "^7.1.0",
		"@babel/plugin-syntax-dynamic-import": "^7.0.0",
		"@babel/plugin-transform-async-to-generator": "^7.1.0",
		"@babel/plugin-transform-runtime": "^7.1.0",
		"@babel/preset-env": "^7.0.0",
		"@babel/register": "^7.0.0",
		"@babel/runtime": "^7.0.0",
		"@frctl/fractal": "^1.1.7",
		"@frctl/mandelbrot": "^1.2.0",
		"@frctl/nunjucks": "^1.0.3",
		"@frctl/twig": "1.0.0-beta.3",
		"ansi-colors": "^3.0.5",
		"autoprefixer": "^9.1.5",
		"babel-eslint": "^9.0.0",
		"babel-loader": "^8.0.2",
		"babel-plugin-ramda": "^1.6.3",
		"commander": "^2.18.0",
		"cssnano": "^4.1.0",
		"deepmerge": "^2.1.1",
		"del": "^3.0.0",
		"devcert-with-localhost": "^0.3.3",
		"eslint": "^5.6.0",
		"eslint-config-airbnb": "^17.1.0",
		"eslint-config-prettier": "^3.0.1",
		"eslint-loader": "^2.1.0",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-jest": "^21.22.0",
		"eslint-plugin-jsx-a11y": "^6.1.1",
		"eslint-plugin-react": "^7.11.1",
		"fancy-log": "^1.3.2",
		"gulp": "^4.0.0",
		"gulp-changed": "^3.2.0",
		"gulp-cssnano": "^2.1.3",
		"gulp-env": "^0.4.0",
		"gulp-html-replace": "^1.6.2",
