{
	"name": "spon-ui",
	"description": "Gulp tasks and Fractal",
	"version": "4.0.0",
	"main": "gulpfile.js",
	"author": "Dave Stockley",
	"license": "MIT",
	"engines": {
		"node": ">=8.6.0"
	},
	"bugs": {
		"url": "https://github.com/magicspon/spon-ui/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/magicspon/spon-ui.git"
	},
	"browser": true,
	"quokka": {
		"babel": {
			"env": "test",
			"configFile": "./.babelrc",
			"polyfill": true
		},
		"installPackageCommand": "yarn add {packageName}",
		"plugins": [
			"jsdom-quokka-plugin"
		],
		"env": {
			"params": {
				"env": "NODE_ENV=test"
			}
		}
	},
	"homepage": "https://github.com/magicspon/spon-ui#readme",
	"scripts": {
		"start": "npx gulp",
		"nasa": "webpack-dashboard npx gulp",
		"cms": "npx gulp --config cms",
		"build:components": "npx gulp build:partials",
		"build": "npx gulp build --env production --config cms",
		"build:fractal": "npx gulp build --env production --config fractal",
		"prebuild:static": "npx gulp build --env production --config fractal --generate static-site",
		"build:static": "npx gulp build:static --config static",
		"deploy": "netlify deploy",
		"preregression:reference": "npx gulp build --env production --config fractal ---backstop reference",
		"regression:reference": "npx gulp regression:test ---backstop reference",
		"preregression:test": "npm run build:fractal",
		"regression:test": "npx gulp regression:test ---backstop test",
		"regression:approve": "npx gulp regression:test ---backstop approve",
		"test": "jest",
		"test:watch": "jest --watch",
		"coverage": "jest --coverage",
		"gen-cert": "mkdir private || node ./scripts/utils/cert/cli.js gen-certs",
		"lint:js": "eslint --ext src/js/** .",
		"lint:scss": "stylelint src/**/*.scss --syntax scss",
		"pretty": "prettier 'src/**/*.{js,jsx,scss}' --write",
		"generate-docs": "npx jsdoc -c jsdoc.json",
		"deploy-docs": "npx jsdoc -c jsdoc.json && cd docs && netlify deploy",
		"cy:run": "cypress run"
	},
	"dependencies": {
		"@barba/core": "^2.6.0",
		"@rematch/core": "^1.2.0",
		"@rematch/persist": "^1.1.6",
		"@spon/connect": "1.0.1",
		"@spon/core": "^2.1.2",
		"@spon/domevents": "^2.0.0",
