		"gulp-newer": "^1.4.0",
		"gulp-notify": "^3.2.0",
		"gulp-postcss": "^8.0.0",
		"gulp-purgecss": "^1.1.1",
		"gulp-rename": "^1.4.0",
		"gulp-sass": "^4.0.1",
		"gulp-sass-glob": "^1.0.9",
		"gulp-sass-variables": "^1.2.0",
		"gulp-sizereport": "^1.2.0",
		"gulp-sourcemaps": "^2.6.4",
		"gulp-stylelint": "^8.0.0",
		"gulp-svg-symbols": "^3.2.0",
		"gulp-svgmin": "^2.0.0",
		"gulp-svgstore": "^7.0.0",
		"htmlhint-stylish": "^1.0.3",
		"jsdoc": "^3.5.5",
		"jsdom-quokka-plugin": "^1.0.13",
		"lodash-webpack-plugin": "^0.11.5",
		"markdown-it": "^8.4.2",
		"markdown-it-abbr": "^1.0.0",
		"markdown-it-footnote": "^3.0.0",
		"modularscale-js": "^3.0.1",
		"modularscale-sass": "^3.0.5",
