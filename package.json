		"require-dir": "^1.0.0",
		"require-glob": "^3.2.0",
		"rucksack-css": "^1.0.2",
		"sass-mq": "^5.0.0",
		"string-hash": "^1.1.3",
		"stylelint": "^9.5.0",
		"stylelint-config-standard": "^18.2.0",
		"stylelint-scss": "^3.3.0",
