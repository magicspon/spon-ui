{
	"name": "spon-ui",
	"description": "Gulp tasks and Fractal",
	"version": "4.0.0",
	"main": "gulpfile.js",
	"author": "Dave Stockley",
	"license": "MIT",
	"engines": {
		"node": ">=8.6.0"
	},
	"bugs": {
		"url": "https://github.com/magicspon/spon-ui/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/magicspon/spon-ui.git"
	},
	"browser": true,
	"jest": {
		"testURL": "http://localhost:3000/",
		"setupFiles": [
			"<rootDir>/setup-jest.js"
		],
		"mapCoverage": true,
		"collectCoverageFrom": [
			"src/js/core/**/*.{js,jsx,ts,tsx}",
			"!**/node_modules/**",
			"!cypress/**"
		],
		"testMatch": [
			"**/?(*.)(spec|test).js?(x)",
			"!cypress/**"
		]
	},
	"quokka": {
		"babel": {
			"env": "test"
		},
		"installPackageCommand": "yarn add {packageName}",
		"plugins": [
			"jsdom-quokka-plugin"
		]
	},
	"homepage": "https://github.com/magicspon/spon-ui#readme",
	"scripts": {
		"start": "npx gulp",
		"nasa": "webpack-dashboard npx gulp",
		"cms": "npx gulp --config cms",
		"build:components": "npx gulp build:partials",
		"build": "npx gulp build --env production --config cms",
		"build:fractal": "npx gulp build --env production --config fractal",
		"prebuild:static": "npx gulp build --env production --config fractal --generate static-site",
		"build:static": "npx gulp build:static --config static",
		"deploy": "netlify deploy",
		"preregression:reference": "npx gulp build --env production --config fractal ---backstop reference",
		"regression:reference": "npx gulp regression:test ---backstop reference",
		"preregression:test": "npm run build:fractal",
		"regression:test": "npx gulp regression:test ---backstop test",
		"regression:approve": "npx gulp regression:test ---backstop approve",
		"test": "jest",
		"test:watch": "jest --watch",
		"coverage": "jest --coverage",
		"gen-cert": "mkdir private || node ./scripts/utils/cert/cli.js gen-certs",
		"lint:js": "eslint --ext src/js/** .",
		"lint:scss": "stylelint src/**/*.scss --syntax scss",
		"pretty": "prettier 'src/**/*.{js,jsx,scss}' --write",
		"generate-docs": "npx jsdoc -c jsdoc.json",
		"deploy-docs": "npx jsdoc -c jsdoc.json && cd docs && netlify deploy"
	},
	"dependencies": {
		"@rematch/core": "^1.0.6",
		"debug": "^4.1.1",
		"deep-object-diff": "^1.1.0",
		"dom-delegate": "^2.2.0",
		"fg-loadcss": "^2.1.0",
		"framesync": "^4.0.1",
		"loadjs": "^3.5.4",
		"mitt": "^1.1.3",
		"picturefill": "^3.0.3",
		"ramda": "^0.26.1",
		"stylefire": "^2.4.0"
	},
	"devDependencies": {
		"@babel/core": "^7.0.1",
		"@babel/plugin-proposal-class-properties": "^7.1.0",
		"@babel/plugin-syntax-dynamic-import": "^7.0.0",
		"@babel/plugin-transform-async-to-generator": "^7.1.0",
		"@babel/plugin-transform-modules-commonjs": "^7.1.0",
		"@babel/plugin-transform-runtime": "^7.1.0",
		"@babel/preset-env": "^7.0.0",
		"@babel/preset-react": "^7.0.0",
		"@babel/register": "^7.0.0",
		"@babel/runtime": "^7.0.0",
		"@frctl/fractal": "^1.1.7",
		"@frctl/mandelbrot": "^1.2.0",
		"@frctl/nunjucks": "^1.0.3",
		"@frctl/twig": "1.0.0-beta.5",
		"ansi-colors": "^3.0.5",
		"autoprefixer": "^9.1.5",
		"babel-eslint": "^10.0.1",
		"babel-loader": "^8.0.2",
		"babel-plugin-lodash": "^3.3.4",
		"babel-plugin-ramda": "^2.0.0",
		"backstopjs": "^3.7.0",
		"browser-sync": "^2.26.3",
		"commander": "^2.18.0",
		"critical": "^1.3.4",
		"cssnano": "^4.1.0",
		"deepmerge": "^3.1.0",
		"del": "^3.0.0",
		"devcert-with-localhost": "^0.3.3",
		"docdash": "^1.0.0",
		"eslint": "^5.6.0",
		"eslint-config-airbnb": "^17.1.0",
		"eslint-config-prettier": "^4.0.0",
		"eslint-loader": "^2.1.0",
		"eslint-plugin-cypress": "^2.0.1",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-jest": "^22.1.3",
		"eslint-plugin-jsx-a11y": "^6.1.1",
		"eslint-plugin-react": "^7.11.1",
		"faker": "^4.1.0",
		"fancy-log": "^1.3.2",
		"gulp": "^4.0.0",
		"gulp-changed": "^3.2.0",
		"gulp-concat": "^2.6.1",
		"gulp-cssnano": "^2.1.3",
