		"vinyl-source-stream": "^2.0.0",
		"webpack": "4.19.1",
		"webpack-bundle-analyzer": "^3.0.2",
		"webpack-config-utils": "^2.3.1",
		"webpack-dev-middleware": "^3.3.0",
		"webpack-hot-middleware": "^2.24.0",
		"webpack-merge": "^4.1.4",
		"webpack-module-hot-accept": "^1.0.5",
		"workbox-webpack-plugin": "^3.5.0",
		"worker-loader": "^2.0.0"
	}
}
