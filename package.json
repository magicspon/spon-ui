		"tailwindcss-visuallyhidden": "^1.0.1",
		"vinyl-buffer": "^1.0.1",
		"vinyl-file": "^3.0.0",
		"vinyl-source-stream": "^2.0.0",
		"webpack": "4.29.0",
		"webpack-bundle-analyzer": "^3.0.3",
		"webpack-config-utils": "^2.3.1",
		"webpack-dashboard": "^2.0.0",
